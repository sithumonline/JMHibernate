<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane alignment="center" hgap="10" minHeight="250.0" minWidth="960.0" prefHeight="250.0" prefWidth="960.0" vgap="10" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.github.sithumonline.view.handler.SQLViewerHandler">
<children>
      <GridPane>
         <children>
            <Label fx:id="lab_info" text="Insert Data" GridPane.columnIndex="1" GridPane.rowIndex="12" />
            <GridPane GridPane.rowIndex="12">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Button fx:id="but_insert" mnemonicParsing="false" onAction="#pressInsert" prefHeight="26.0" prefWidth="174.0" text="Insert" GridPane.rowIndex="1" />
                  <Button fx:id="but_update" mnemonicParsing="false" onAction="#pressUpdate" prefHeight="26.0" prefWidth="174.0" text="Update" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <Button fx:id="but_delete" mnemonicParsing="false" onAction="#pressDelete" prefHeight="26.0" prefWidth="174.0" text="Delete" GridPane.rowIndex="2" />
                  <Button fx:id="but_clear" mnemonicParsing="false" onAction="#pressClear" prefHeight="26.0" prefWidth="174.0" text="Clear" GridPane.columnIndex="1" GridPane.rowIndex="2" />
               </children>
            </GridPane>
            <Label fx:id="lab_msg" text="username" GridPane.rowIndex="5" />
            <TextField fx:id="txt_username" GridPane.rowIndex="6" />
            <Label fx:id="lab_msg1" text="password" GridPane.rowIndex="8" />
            <TextField fx:id="txt_password" GridPane.rowIndex="9" />
            <Label fx:id="lab_msg11" text="fullname" GridPane.columnIndex="1" GridPane.rowIndex="5" />
            <TextField fx:id="txt_fullname" GridPane.columnIndex="1" GridPane.rowIndex="6" />
            <Label fx:id="lab_msg111" text="email" GridPane.columnIndex="1" GridPane.rowIndex="8" />
            <TextField fx:id="txt_email" GridPane.columnIndex="1" GridPane.rowIndex="9" />
         </children>
         <columnConstraints>
            <ColumnConstraints />
            <ColumnConstraints />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints />
            <RowConstraints />
            <RowConstraints />
            <RowConstraints />
            <RowConstraints />
            <RowConstraints />
            <RowConstraints />
            <RowConstraints />
            <RowConstraints />
            <RowConstraints />
            <RowConstraints />
            <RowConstraints />
            <RowConstraints />
         </rowConstraints>
      </GridPane>
      <TableView fx:id="tab_user" prefHeight="200.0" prefWidth="528.0" GridPane.columnIndex="3">
        <columns>
          <TableColumn fx:id="tab_userid" prefWidth="75.0" text="user_id" />
          <TableColumn fx:id="tab_username" prefWidth="90.0" text="username" />
            <TableColumn fx:id="tab_password" prefWidth="87.0" text="password" />
            <TableColumn fx:id="tab_fullname" minWidth="0.0" prefWidth="106.0" text="fullname" />
            <TableColumn fx:id="tab_email" prefWidth="167.0" text="email" />
        </columns>
      </TableView>
   </children>
   <columnConstraints>
      <ColumnConstraints />
      <ColumnConstraints />
      <ColumnConstraints />
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
</GridPane>