<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.github.sithumonline.view.handler.UserLoginViewHandler">
   <children>
      <TextField fx:id="txtUsername" layoutX="305.0" layoutY="99.0" />
      <TextField fx:id="txtPassword" layoutX="305.0" layoutY="145.0" />
      <Button fx:id="butLogin" layoutX="305.0" layoutY="187.0" mnemonicParsing="false" onAction="#pressLogin" prefHeight="26.0" prefWidth="171.0" text="Login" />
      <Button fx:id="butCreateAccount" layoutX="305.0" layoutY="229.0" mnemonicParsing="false" onAction="#pressCreateAccount" prefHeight="26.0" prefWidth="171.0" text="Create Account" />
      <Button fx:id="butForgetPassword" layoutX="305.0" layoutY="270.0" mnemonicParsing="false" onAction="#pressForgetPassword" prefHeight="26.0" prefWidth="171.0" text="Forget Password" />
      <Label layoutX="305.0" layoutY="76.0" text="Username" />
      <Label layoutX="305.0" layoutY="125.0" text="Password" />
      <Label fx:id="labInfo" layoutX="305.0" layoutY="311.0" prefHeight="16.0" prefWidth="290.0" />
   </children>
</AnchorPane>
